####### Compile

.obj/main.o: main.cpp .ui/admin.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/main.o main.cpp

.ui/admin.h: admin.ui 
	$(UIC) admin.ui -o .ui/admin.h

.ui/admin.cpp: .ui/admin.h admin.ui 
	$(UIC) admin.ui -i admin.h -o .ui/admin.cpp

.ui/io.h: io.ui 
	$(UIC) io.ui -o .ui/io.h

.ui/io.cpp: .ui/io.h io.ui 
	$(UIC) io.ui -i io.h -o .ui/io.cpp

.ui/addrem.h: addrem.ui 
