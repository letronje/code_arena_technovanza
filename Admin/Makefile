####### Build rules

all: Makefile $(TARGET)

$(TARGET): $(UICDECLS) $(OBJECTS) $(OBJMOC) 
	$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJMOC) $(LIBS)

mocables: $(SRCMOC)

$(MOC): 
	( cd $(QTDIR)/src/moc ; $(MAKE) )

Makefile: admin.pro  $(QTDIR)/mkspecs/default/qmake.conf 
	$(QMAKE) -o Makefile admin.pro
qmake: 
	@$(QMAKE) -o Makefile admin.pro

dist: 
	@mkdir -p .obj/admin && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .obj/admin/ && $(COPY_FILE) --parents admin.ui.h io.ui.h addrem.ui.h .obj/admin/ && ( cd `dirname .obj/admin` && $(TAR) admin.tar admin && $(GZIP) admin.tar ) && $(MOVE) `dirname .obj/admin`/admin.tar.gz . && $(DEL_FILE) -r .obj/admin

mocclean:
	-$(DEL_FILE) $(OBJMOC)
	-$(DEL_FILE) $(SRCMOC)

uiclean:
	-$(DEL_FILE) $(UICIMPLS) $(UICDECLS)

yaccclean:
lexclean:
clean: mocclean uiclean
