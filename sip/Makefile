####### Compiler, tools and options

CC       = gcc
CXX      = g++
LEX      = flex
YACC     = yacc
CFLAGS   = -pipe -Wall -W -O2 -march=i386 -mcpu=i686 -g -DGLX_GLXEXT_LEGACY -fno-use-cxa-atexit -fno-exceptions  -DQT_NO_DEBUG
CXXFLAGS = -pipe -Wall -W -O2 -march=i386 -mcpu=i686 -g -DGLX_GLXEXT_LEGACY -fno-use-cxa-atexit -fno-exceptions  -DQT_NO_DEBUG
LEXFLAGS = 
YACCFLAGS= -d
INCPATH  = -I$(QTDIR)/mkspecs/default -I. -I$(QTDIR)/include -I.ui/ -I.moc/
LINK     = g++
LFLAGS   = 
LIBS     = $(SUBLIBS) -L$(QTDIR)/lib -L/usr/X11R6/lib -lqt-mt -lXext -lX11 -lm
AR       = ar cqs
RANLIB   = 
MOC      = $(QTDIR)/bin/moc
UIC      = $(QTDIR)/bin/uic
QMAKE    = qmake
TAR      = tar -cf
GZIP     = gzip -9f
COPY     = cp -f
COPY_FILE= $(COPY) -p
COPY_DIR = $(COPY) -pR
DEL_FILE = rm -f
SYMLINK  = ln -sf
DEL_DIR  = rmdir
MOVE     = mv -f
CHK_DIR_EXISTS= test -d
MKDIR    = mkdir -p

####### Output directory

OBJECTS_DIR = .obj/

####### Files

HEADERS = 
SOURCES = main.cpp
OBJECTS = .obj/main.o \
		.obj/sip.o
FORMS = sip.ui
UICDECLS = .ui/sip.h
UICIMPLS = .ui/sip.cpp
SRCMOC   = .moc/moc_sip.cpp
OBJMOC = .obj/moc_sip.o
DIST	   = sip.pro
QMAKE_TARGET = sip
DESTDIR  = 
TARGET   = sip

first: all
####### Implicit rules

.SUFFIXES: .c .cpp .cc .cxx .C

.cpp.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.cc.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.cxx.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.C.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.c.o:
	$(CC) -c $(CFLAGS) $(INCPATH) -o $@ $<

####### Build rules

all: Makefile $(TARGET)

$(TARGET): $(UICDECLS) $(OBJECTS) $(OBJMOC) 
	$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJMOC) $(LIBS)

mocables: $(SRCMOC)

$(MOC): 
	( cd $(QTDIR)/src/moc ; $(MAKE) )

Makefile: sip.pro  $(QTDIR)/mkspecs/default/qmake.conf 
	$(QMAKE) -o Makefile sip.pro
qmake: 
	@$(QMAKE) -o Makefile sip.pro

dist: 
	@mkdir -p .obj/sip && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .obj/sip/ && $(COPY_FILE) --parents sip.ui.h .obj/sip/ && ( cd `dirname .obj/sip` && $(TAR) sip.tar sip && $(GZIP) sip.tar ) && $(MOVE) `dirname .obj/sip`/sip.tar.gz . && $(DEL_FILE) -r .obj/sip

mocclean:
	-$(DEL_FILE) $(OBJMOC)
	-$(DEL_FILE) $(SRCMOC)

uiclean:
	-$(DEL_FILE) $(UICIMPLS) $(UICDECLS)

yaccclean:
lexclean:
clean: mocclean uiclean
	-$(DEL_FILE) $(OBJECTS) 
	-$(DEL_FILE) *~ core *.core


####### Sub-libraries

distclean: clean
	-$(DEL_FILE) $(TARGET) $(TARGET)


FORCE:

####### Compile

.obj/main.o: main.cpp .ui/sip.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/main.o main.cpp

.ui/sip.h: sip.ui 
	$(UIC) sip.ui -o .ui/sip.h

.ui/sip.cpp: .ui/sip.h sip.ui 
	$(UIC) sip.ui -i sip.h -o .ui/sip.cpp

.obj/sip.o: .ui/sip.cpp sip.ui.h \
		.ui/sip.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/sip.o .ui/sip.cpp

.obj/moc_sip.o: .moc/moc_sip.cpp .ui/sip.h 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/moc_sip.o .moc/moc_sip.cpp

.moc/moc_sip.cpp: $(MOC) .ui/sip.h
	$(MOC) .ui/sip.h -o .moc/moc_sip.cpp

####### Install

install: all 

uninstall: 

